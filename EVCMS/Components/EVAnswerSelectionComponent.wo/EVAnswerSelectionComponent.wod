SelectionElement : EVToOneRelationship {
	uiStyle = toOneUIStyle;
	sourceObject = value;
	relationshipKey = "selectedOption";
	destinationDisplayKey = "optionName";
	possibleChoices = dataSetItem.options;
	isMandatory = value.dataSetItem.isRequired;
	destinationSortKey = "orderNumber";
	maxColumns = dataSetItem.options.count;
}